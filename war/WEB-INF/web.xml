<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
              http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5"
         xmlns="http://java.sun.com/xml/ns/javaee">

  <!-- Servlets -->
  <servlet>
    <servlet-name>greetServlet</servlet-name>
    <servlet-class>com.rhuan.server.GreetingServiceImpl</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>greetServlet</servlet-name>
    <url-pattern>/logingwt/greet</url-pattern>
  </servlet-mapping>
  
  <!--Fim -->
    <!-- Servlets Login -->
    <servlet>
    <servlet-name>loginServlet</servlet-name>
    <servlet-class>com.rhuan.server.LoginServiceImpl</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>loginServlet</servlet-name>
    <url-pattern>/logingwt/login</url-pattern>
  </servlet-mapping>
   <!--Fim -->
    <!-- Servlets Cadastro -->
    <servlet>
    <servlet-name>CadastroServlet</servlet-name>
    <servlet-class>com.rhuan.server.CadastroServiceImpl</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>CadastroServlet</servlet-name>
    <url-pattern>/logingwt/cadastro</url-pattern>
  </servlet-mapping>
   <!--Fim -->
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>LoginGWT.html</welcome-file>
  </welcome-file-list>

</web-app>
